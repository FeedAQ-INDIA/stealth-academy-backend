        {
          "name": "Google OAuth Login",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/auth/google",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "google"],
              "query": [
                {
                  "key": "redirectUri",
                  "value": "{{frontendUrl}}/",
                  "description": "Optional redirect URI after successful login"
                }
              ]
            }
          }
        },
        {
          "name": "Microsoft OAuth Login",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/auth/microsoft",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "microsoft"],
              "query": [
                {
                  "key": "redirectUri",
                  "value": "{{frontendUrl}}/",
                  "description": "Optional redirect URI after successful login"
                }
              ]
            }
          }
        },
        {
          "name": "Refresh Token",
          "request": {
            "method": "POST",
            "url": {
              "raw": "{{baseUrl}}/auth/refresh-token",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "refresh-token"]
